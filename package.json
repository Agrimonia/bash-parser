{
  "name": "bash-parser",
  "version": "0.3.2",
  "main": "src/index.js",
  "description": "Standard compliant bash parser",
  "repository": "vorpaljs/bash-parser",
  "license": "MIT",
  "author": "vorpal-js",
  "scripts": {
    "test": "ava -c 5 && xo --ignore **/built-grammar.js --ignore 'packages/**/*'",
    "build": "mgb ./src/modes posix && mgb ./src/modes bash",
    "cover-test": "nyc ava --require babel-register && xo --ignore **/built-grammar.js --ignore 'packages/**/*'",
    "build-site": "browserify docs/index.js -o docs/bundle.js -t [ babelify --presets [ es2015 ] ] -t uglifyify"
  },
  "keywords": [],
  "engines": {
    "node": ">=4"
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ],
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "callsite": "^1.0.0",
    "coveralls": "^2.11.11",
    "jison": "^0.4.17",
    "json5": "^0.5.0",
    "mode-grammar-builder": "file:packages/mode-grammar-builder",
    "nyc": "^7.0.0",
    "rus-diff": "^1.1.0",
    "tree-walk": "^0.4.0",
    "uglifyify": "^3.0.2",
    "xo": "^0.16.0"
  },
  "files": [
    "src"
  ],
  "nyc": {
    "exclude": [
      "src/modes/posix/built-grammar.js",
      "test/_utils.js"
    ]
  },
  "dependencies": {
    "array-last": "^1.1.1",
    "babylon": "^6.9.1",
    "compose-function": "^3.0.3",
    "deep-freeze": "0.0.1",
    "filter-obj": "^1.1.0",
    "has-own-property": "^0.1.0",
    "identity-function": "^1.0.0",
    "iterable-lookahead": "^1.0.0",
    "iterable-replace": "file:packages/iterable-replace",
    "magic-string": "^0.16.0",
    "map-iterable": "^1.0.1",
    "map-obj": "^2.0.0",
    "object-pairs": "^0.1.0",
    "object-values": "^1.0.0",
    "shell-quote-word": "^1.0.1",
    "unescape-js": "^1.0.5"
  }
}
